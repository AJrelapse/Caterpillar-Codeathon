    <div class="px-4 py-5 my-5 text-center">
        <div class="col-lg-6 mx-auto">
          <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
            <div class="form-container pack">
                <p class="title">ENGINE</p>
                <form class="form" id="configpage">
                    <div class="input-group">
                        <label >Speed (rpm)</label>
                        <input type="text" name="username" id="Speed" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Temperature (°C)</label>
                        <input type="text" name="username" id="Temperature" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Oil Pressure (psi)</label>
                        <input type="text" name="password" id="Oil-Pressure" placeholder="">
                    </div>
        
                    <p class="title">FUEL</p>
                
                    <div class="input-group">
                        <label >Water in fuel (ppm)</label>
                        <input type="text" name="username" id="Water" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Temperature (°C)</label>
                        <input type="text" name="username" id="temp" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Pressure (psi)</label>
                        <input type="text" name="password" id="Pressure" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Level (%)</label>
                        <input type="text" name="password" id="Level" placeholder="">
                    </div>
        
                    <p class="title">DRIVE</p>
                
                    <div class="input-group">
                        <label >Transmission Pressure (psi)</label>
                        <input type="text" name="username" id="Transmission" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Pedal Sensor (V)</label>
                        <input type="text" name="username" id="Sensor" placeholder="">
                    </div>
                    <div class="input-group">
                        <label>Brake Control (psi)</label>
                        <input type="text" name="password" id="Brake" placeholder="">
                    </div>
        
                    <p class="title">MISC</p>
                
                    <div class="input-group">
                        <label >Hydraulic Pump Rate (Lit/min)</label>
                        <input type="text" name="username" id="Hydraulic" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Exhaust Gas Temperature (°C)</label>
                        <input type="text" name="username" id="Exhaust" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >System Voltage (V)</label>
                        <input type="text" name="password" id="Voltage" placeholder="">
                    </div>
                    <div class="input-group">
                        <label >Air Filter Pressure (inches of water column)</label>
                        <input type="text" name="password" id="Filter" placeholder="">
                    </div>
                    <p class="title"> </p>
        
                    <button class="sign"><a href="/solution" style="color: black;">Submit</a></button>
                </form>
            </div>
          </div>
        </div>
      </div>

    


