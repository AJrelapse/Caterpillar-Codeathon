<body>

  <div id="myCarousel"></div>

<section class="et-hero-tabs">
    <div class="et-hero-tab-contain">
      <a class="et-hero-tab" href="/"><img class="logo" src="public/images/Caterpillar_logo.svg.png"></a>
      <a class="et-hero-tab" href="/">History</a>
